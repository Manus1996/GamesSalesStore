<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:GamesSalesStore"
             x:Class="GamesSalesStore.AppShell">

    <FlyoutItem Title="First Page">
        <ShellContent>
            <local:MainPage/>
        </ShellContent>
    </FlyoutItem>

    <FlyoutItem Title="Games">
        <ShellContent>
            <local:GamesList/>
        </ShellContent>
    </FlyoutItem>

    <FlyoutItem Title="Purchase">
        <ShellContent>
            <local:PurchasePage/>
        </ShellContent>
    </FlyoutItem>

    <FlyoutItem Title="Order List">
        <ShellContent>
            <local:OrderList/>
        </ShellContent>
    </FlyoutItem>

    <FlyoutItem Title="FAQ">
        <ShellContent>
            <local:FAQ/>
        </ShellContent>
    </FlyoutItem>
</Shell>